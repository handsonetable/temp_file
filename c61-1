							              /* $.each(result, function(k,v) {
											 
											    console.log(v);
											  
											});
							                var myresult=result;
							*/




							            };
							            reader.readAsArrayBuffer(f);
							        }

					        }
					        else{

					        	 alert('invalid extension!');
					        }
		   
		    }

  //Change event to dropdownlist
		  $(document).ready(function(){
		    $('#files').change(handleFile);
